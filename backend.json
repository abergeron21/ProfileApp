{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[0-9a-zA-Z_-]+$": {
      "type": "object",
      "properties": {
        "age": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "major": {
          "type": "string"
        },
        "movie": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["age", "image", "major", "movie", "name"]
    }
  }
}
